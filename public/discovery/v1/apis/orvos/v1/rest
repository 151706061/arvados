  "CommentReply": {
   "id": "CommentReply",
   "type": "object",
   "description": "A JSON representation of a reply to a comment on a file in Google Orvos.",
   "properties": {
    "author": {
     "$ref": "User",
     "description": "The user who wrote this reply."
    },
    "content": {
     "type": "string",
